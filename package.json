{
    "name": "mockingbird-proxy",
    "version": "1.0.0",
    "description": "",
    "main": "./src/main.js",
    "scripts": {
        "start": "electron .",
        "start:dev": "npm-run-all --parallel sass:electron start",
        "sass:express": "sass --watch src/public/scss:src/public/styles",
        "sass:electron": "sass --watch src/assets/scss:src/assets/styles",
        "build": "electron-builder --all",
        "build:macos": "electron-builder --macos --x64",
        "build:win": "electron-builder --win --x64",
        "build:linux": "electron-builder --linux --x64"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "bootstrap": "^5.3.2",
        "bootstrap-icons": "^1.11.2",
        "cors": "^2.8.5",
        "crypto": "^1.0.1",
        "ejs": "^3.1.9",
        "electron-store": "^8.1.0",
        "express": "^4.18.2",
        "express-handlebars": "^7.1.2",
        "express-http-proxy": "^2.0.0",
        "http-proxy": "^1.18.1",
        "http-proxy-middleware": "^3.0.0-beta.1",
        "jquery": "^3.7.1",
        "jsdom": "^23.0.1",
        "jsoneditor": "^9.10.4",
        "node-cache": "^5.1.2",
        "npm-run-all": "^4.1.5",
        "popper.js": "^1.16.1",
        "request": "^2.88.2",
        "sass": "^1.69.5",
        "vanilla-jsoneditor": "^0.20.0"
    },
    "devDependencies": {
        "babel-loader": "^9.1.3",
        "electron": "^28.0.0",
        "electron-builder": "^24.9.1",
        "electron-packager": "^17.1.2",
        "nodemon": "^3.0.1"
    },
    "build": {
        "appId": "tuo.id.app",
        "portable": {
            "artifactName": "${productName}-portable-${version}.${ext}"
        },
        "directories" :{
            "buildResources": "./buildResources"
        }
    }
}
